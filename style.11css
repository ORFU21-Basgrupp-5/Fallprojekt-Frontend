#welcome_image{
  width: 300px;
  display: grid;
}
.gridstyle.container {
  display: grid;
  height: 100vh;
  grid-template-rows: auto auto 1fr auto auto auto;
  gap: 24px;
  color: #535353;
  
}
a:link {
  color: #838E83;
}
a:visited {
  color: #838E83;

}
a:hover {
  color: #b63e23;
}
a:active {
  color: #838E83;
}
#navbar {
  list-style-type: none;
  margin: 0;
  padding: 0;
  overflow: hidden;
  display: inline-flex;

  justify-content: flex-start;
  flex-wrap: wrap;


}


.navigation-menu{
  /* margin: 0 auto; */
  font-size: 20px;
  display:flex;
  flex-wrap: wrap;
  justify-content: flex-end;
  flex-direction: row;
  justify-content: space-between;



}

ul#navbar li a {
  display: block;
  color: #535353;
  font-weight:bold;
  padding: 8px 16px;
  text-decoration: none;
  border-radius:5px;
  cursor:pointer;
  /* box-shadow: 0 0 5px #000000; */
  margin: 3px;
}
ul#navbar li a:hover {
  background-color: #838E83;
  color: #535353;
}
.site-footer{
  list-style-type: none;
  margin: 0;
  padding: 0;
  overflow: hidden;
  display: flex;

  justify-content: space-evenly;
  flex-wrap: wrap;
}
.socials{
  list-style-type: none;
}
.socials a{
  text-decoration: none;
  color: #535353;
}

