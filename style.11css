background-color: #B2BCAA;
 

    
  

}
@media (min-width: 720px) {
 
  
  .gridstyle .header {
    grid-column: 1 / span 12;
    
  }

 
  .gridstyle .navigation {
    grid-column: 1 / span 12;
    
  }

  
  .gridstyle .main {
    grid-column: 3 / span 10;
  }

  
  .gridstyle .sidebar {
    
    grid-column: 1 / span 2;
    grid-row: 3;
  }

  
  .gridstyle .ads {
    
    grid-column: 1 / span 2;
  }

 
  .gridstyle .footer {
    
    grid-column: 3 / span 10;
  }
}

@media (min-width: 1020px) {

  
  .gridstyle .navigation {
    grid-column: 3 / span 8;
    
  }

  
  .gridstyle .main {
    grid-column: 3 / span 8;
    
  }

  
  .gridstyle .sidebar {
    grid-row: 2 / 4;
    
  }

  
  .gridstyle .ads {
    
    grid-column: 11 / span 2;
    grid-row: 2 / 4;
  }

  
  .gridstyle .footer {
    
    grid-column: 1 / span 12;
  }
}

